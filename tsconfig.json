{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "useDefineForClassFields": true,
        "baseUrl": ".",
        "typeRoots": [
            "./node_modules/ionic-svelte"
        ],
        "types": [
            "ionic-svelte"
        ],
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true
    },
    "references": [{ "path": "./tsconfig.node.json" }]
}
