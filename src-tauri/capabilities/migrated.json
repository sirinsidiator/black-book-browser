{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-stat",
    {
      "identifier": "fs:scope",
      "allow": [
        "**"
      ]
    },
    "dialog:allow-open",
    "dialog:default",
    "fs:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "**"
        }
      ]
    }
  ]
}