{
    "name": "black-book-browser",
    "private": true,
    "version": "4.2.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin prettier-plugin-svelte . && eslint .",
        "format": "prettier --write --plugin prettier-plugin-svelte .",
        "tauri": "tauri",
        "update-patrons": "node scripts/update-patrons.js",
        "actions": "act --artifact-server-path ./artifacts --env CACHE_BIN=false"
    },
    "dependencies": {
        "@ionic/core": "^8.6.5",
        "@tauri-apps/api": "^2.7.0",
        "@tauri-apps/plugin-dialog": "^2.3.1",
        "@tauri-apps/plugin-fs": "^2.4.1",
        "@tauri-apps/plugin-opener": "^2.4.0",
        "fuzzysort": "^3.1.0",
        "highlight.js": "^11.11.1",
        "ionic-svelte": "^0.5.85",
        "ionicons": "^8.0.13",
        "picomatch": "^4.0.3",
        "svelte-virtual-list-ce": "^3.1.0-beta.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.25.1",
        "@sveltejs/vite-plugin-svelte": "^6.1.0",
        "@tauri-apps/cli": "^2.7.1",
        "@tsconfig/svelte": "^5.0.4",
        "@types/picomatch": "^4.0.1",
        "eslint": "^9.31.0",
        "eslint-plugin-svelte": "^3.11.0",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte": "^5.36.13",
        "svelte-check": "^4.3.0",
        "svelte-preprocess": "^6.0.3",
        "svelte-preprocess-import-assets": "^1.1.0",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0",
        "vite": "^7.0.5"
    }
}
